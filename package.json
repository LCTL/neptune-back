{
  "name": "dmm-backend",
  "version": "0.0.1",
  "description": "Docker Machine Manager (Backend)",
  "main": "/dist/app.js",
  "scripts": {
    "setup": "npm install && node node_modules/tsd/build/cli.js install && node node_modules/tsd/build/cli.js link",
    "build": "npm run setup && node node_modules/typescript/bin/tsc",
    "babel-node": "node node_modules/babel-cli/bin/babel-node dist/app.js",
    "dev": "node node_modules/nodemon/bin/nodemon --watch ./dist --exec npm run babel-node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "docker",
    "maanger"
  ],
  "author": "Lawrence Cheung",
  "license": "Apache Licence 2.0",
  "dependencies": {
    "koa": "^1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-trie-router": "^1.0.10",
    "nodedm": "^0.0.3"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.1.18",
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "tsd": "^0.6.5",
    "typescript": "next"
  }
}
